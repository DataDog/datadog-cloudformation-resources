{
    "typeName": "Datadog::IAM::User",
    "description": "Datadog Application User",
    "properties": {
        "DatadogCredentials": {
            "description": "Credentials for the Datadog API",
            "properties": {
                "ApiKey": {
                    "description": "Datadog API key",
                    "type": "string"
                },
                "ApplicationKey": {
                    "description": "Datadog application key",
                    "type": "string"
                }
            },
            "required": ["ApiKey", "ApplicationKey"],
            "type": "object"
        },
        "AccessRole": {
            "description": "The role of the user",
            "enum": [
                "adm",
                "ro",
                "st"
            ],
            "type": "string"
        },
        "Disabled": {
            "description": "Wheter or not the user is disabled",
            "type": "boolean"
        },
        "Email": {
            "description": "User email",
            "type": "string"
        },
        "Handle": {
            "description": "User handle (a valid email)",
            "type": "string"
        },
        "Name": {
            "description": "User name",
            "type": "string"
        },
        "Verified": {
            "description": "Whether or not the user is verified",
            "type": "boolean"
        }
    },
    "createOnlyProperties": [
        "/properties/Handle"
    ],
    "primaryIdentifier": [
        "/properties/Handle"
    ],
    "readOnlyProperties": [
        "/properties/Disabled", "/properties/Verified"
    ],
    "required": [
        "DatadogCredentials"
    ],
    "additionalProperties": false
}
