{
  "Type": "log alert",
  "Query": "logs(\"source:syslog @facility_name:authpriv.* env:prod @tag:(-CRON* -systemd* -sshd* -sudo* -useradd* -userdel)\").index(\"main\").rollup(\"count\").last(\"1h\") >= 1",
  "Name": "SSH Activity on PROD",
  "Message": "@slack-datadog-testing",
  "Options": {
    "Locked": false,
    "IncludeTags": true,
    "NewHostDelay": 300,
    "NotifyAudit": false,
    "NotifyNoData": false,
    "RenotifyInterval": 0,
    "EnableLogsSample": true,
    "Thresholds": {
      "Critical": 1
    }
  }
}