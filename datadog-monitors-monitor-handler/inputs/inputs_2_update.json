{
  "Type": "rum alert",
  "Query": "rum(\"*\").rollup(\"count\").last(\"5m\") > 5000000",
  "Name": "RUM Monitoring on PROD updated",
  "Message": "@slack-datadog-testing",
  "Priority": 1,
  "Options": {
    "Locked": false,
    "IncludeTags": false,
    "NewHostDelay": 40,
    "NotifyAudit": true,
    "NotifyNoData": true,
    "RenotifyInterval": 5,
    "Thresholds": {
      "Critical": 5000000
    }
  }
}
